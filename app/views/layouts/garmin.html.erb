<%= extends :base do %>
  <%= append :head do %>
    <style type="text/css" media="all">@import "communicator-api/garmin/device/style/communicator2.css";</style>
  <% end %>
  <% replace :body_class, 'authenticated garmin' %>
  <% replace :body_data_attributes, 'data-no-turbolink="true"' %>
  <% append :header_masthead, render(partial: 'shared/header_authenticated') %>
  <% replace :content_inner, yield %>
  <%= append :js do %>
    <%= javascript_include_tag "garmin", "data-turbolinks-track" => true %>
    <script type="text/javascript" src="communicator-api/garmin/device/GarminDeviceDisplay.js">&#160;</script>
  <% end %>
<% end %>
